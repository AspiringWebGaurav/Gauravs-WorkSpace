{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-size-adjust",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "css-prefix-order": "off"
  }
}